/* pages/departmentList/departmentList.wxss */
.searchBox{
  padding:28rpx 30rpx;
  border-radius: 10rpx;
  background: #c5cbce;
  display: flex;
  align-items: center;
  justify-content: flex-start;
          
}
.searchBox input{font-size: 28rpx;}

.finishList{padding:0 30rpx;
            border-radius: 10rpx;
            background: #fff;
            margin-top: 30rpx;
           
}
.finishListsub{display: flex;
               align-content:space-between;
               align-items:center;
               padding: 30rpx 30rpx 20rpx 0rpx;
               border-bottom: 1rpx solid #e6e6e6;
               font-size: 34rpx;
               position: relative;
               
}
.deparsub{
  display:flex;
  flex-direction:column;
  align-items:stretch;
}


/* 操作列表 */
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 99;
  color: #fff; 
}
.optionlist{
  width: 700rpx;
  margin: 0 auto;
  z-index: 99;
  position: fixed;
  bottom: 40rpx;
}
.option{
  padding:30rpx;
  display: flex;
  justify-content:center;
  align-items:center;
  background: #fff;
  font-size: 32rpx;
  border-bottom:1rpx solid #e6e6e6;
}