.nameCompany{
  padding: 30rpx 20rpx;
  height:40rpx;
  font-size: 36rpx;
  background: #fff;
  border-radius: 10rpx;
  display: flex;
  align-content:space-between;
  align-items:center;

}

.nameinput{
  width: 400rpx;
  height: 40rpx;
}

.finishList{padding:0 30rpx;
            border-radius: 10rpx;
            background: #fff;
            margin-top: 30rpx;
           
}
.finishListsub{display: flex;
               align-content:space-between;
               align-items:center;
               padding: 30rpx 0;
               border-bottom: 1rpx solid #e6e6e6;
               font-size: 34rpx;
               position: relative;
               
}
.finishListsub:after{
                      content: "";
                      position: absolute;
                      left: -15rpx;
                      top: 45rpx;
                      width: 0;
                      height: 0;
                      border-left: 10rpx solid #b3b3b3;
                      border-top: 8rpx solid transparent;
                      border-bottom: 8rpx solid transparent;
}
.manageDepar{display: flex;
             align-content:space-between;
             align-items:center;
             padding:30rpx;
             border-radius: 10rpx;
             background: #fff;
             margin-top: 30rpx;

}